{
  "name": "roq/binance_futures/flags/REST",
  "type": "flags",
  "prefix": "rest_",
  "values": [
    {
      "name": "uri",
      "type": "roq/io/web/URI",
      "validator": "roq/core/flags/URI",
      "default": "https://testnet.binancefuture.com",
      "description": "Exchange end-point"
    },
    {
      "name": "host",
      "type": "std/string",
      "description": "Host (when URI is an IP address)"
    },
    {
      "name": "pm_uri",
      "type": "roq/io/web/URI",
      "validator": "roq/core/flags/URI",
      "description": "Exchange end-point (portfolio margin)"
    },
    {
      "name": "pm_host",
      "type": "std/string",
      "description": "Host (when URI is an IP address)"
    },
    {
      "name": "proxy",
      "type": "roq/io/web/URI",
      "validator": "roq/core/flags/URI",
      "description": "Proxy end-point"
    },
    {
      "name": "ping_freq",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "5s",
      "description": "Ping frequency"
    },
    {
      "name": "ping_path",
      "type": "std/string",
      "validator": "roq/flags/validators/Path",
      "default": "/v1/time",
      "description": "Ping path"
    },
    {
      "name": "request_timeout",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "30s",
      "description": "Request timeout"
    },
    {
      "name": "listen_key_refresh",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "1800s",
      "description": "Listen key refresh period"
    },
    {
      "name": "order_recv_window",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "5000ms",
      "description": "Receive window (please refer to exchange documentation)"
    },
    {
      "name": "cancel_on_disconnect",
      "type": "std/bool",
      "default": false,
      "description": "Auto-cancel orders on disconnect?"
    },
    {
      "name": "order_countdown",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "30s",
      "description": "Auto-cancel countdown period"
    },
    {
      "name": "terminate_on_403",
      "type": "std/bool",
      "default": false,
      "description": "(TEST) Trigger program termination after receiving a 403 response code?"
    },
    {
      "name": "back_off_delay",
      "type": "std/nanoseconds",
      "validator": "roq/flags/validators/TimePeriod",
      "default": "300s",
      "description": "(TEST) Time to wait before retrying a rejected request"
    },
    {
      "name": "drop_order_update",
      "type": "std/bool",
      "default": false,
      "description": "(TEST) Drop order updates from REST?"
    }
  ]
}
